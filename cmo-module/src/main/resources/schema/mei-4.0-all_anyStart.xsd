         </xs:annotation>
         <xs:simpleType>
            <xs:restriction base="xs:string">
               <xs:pattern value="\d+(\.\d+)?(\s*\+\s*\d+(\.\d+)?)*"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
   </xs:attributeGroup>
   <xs:attributeGroup name="mei_att.meterSigDefault.log.attribute.meter.unit">
