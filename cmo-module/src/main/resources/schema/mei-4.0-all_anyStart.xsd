   <xs:element name="li">
      <xs:complexType mixed="true">
         <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:group ref="mei:mei_model.textComponentLike"/>
            <xs:group ref="mei:mei_model.textPhraseLike"/>
            <xs:element ref="mei:mei_model.editLike"/>
