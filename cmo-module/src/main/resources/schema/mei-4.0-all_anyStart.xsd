         <xs:annotation>
            <xs:documentation>Indicates the use of a meter symbol instead of a numeric meter signature, that is, 'C' for common time or 'C' with a slash for cut time.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:attributeGroup>
   <xs:attributeGroup name="mei_att.meterSig.log.attribute.unit">
      <xs:attribute name="unit" type="xs:decimal">
