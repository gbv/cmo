         <xs:attributeGroup ref="mei:mei_att.facsimile.attributes"/>
         <xs:attributeGroup ref="mei:mei_att.lang.attributes"/>
         <xs:attributeGroup ref="mei:mei_att.metadataPointing.attributes"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="back" substitutionGroup="mei:mei_model.backLike">
      <xs:annotation>
         <xs:documentation>(back matter) – Contains any appendixes, advertisements, indexes, etc. following the main body of a musical text.</xs:documentation>
      </xs:annotation>
   </xs:element>
   <xs:element name="epigraph">
