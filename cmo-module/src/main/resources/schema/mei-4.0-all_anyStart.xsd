                  <xs:simpleType>
                     <xs:restriction base="xs:token">
                        <xs:enumeration value="alternative">
                           <xs:annotation>
                              <xs:documentation>Alternate title by which the item is also known.</xs:documentation>
                           </xs:annotation>
                        </xs:enumeration>
                     </xs:restriction>
                  </xs:simpleType>
                  <xs:simpleType>
                     <xs:restriction base="xs:token">
                        <xs:enumeration value="translated">
                           <xs:annotation>
                              <xs:documentation>Translated form of title.</xs:documentation>
                           </xs:annotation>
                        </xs:enumeration>
                     </xs:restriction>
                  </xs:simpleType>
                  <xs:simpleType>
                     <xs:restriction base="xs:token">
