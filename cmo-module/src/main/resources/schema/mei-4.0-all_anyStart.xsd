   <xs:attributeGroup name="mei_att.stringtab.attributes">
      <xs:attributeGroup ref="mei:mei_att.stringtab.attribute.tab.fing"/>
      <xs:attributeGroup ref="mei:mei_att.stringtab.attribute.tab.fret"/>
      <xs:attributeGroup ref="mei:mei_att.stringtab.attribute.tab.string"/>
   </xs:attributeGroup>
   <xs:attributeGroup name="mei_att.stringtab.attribute.tab.fing">
      <xs:attribute name="tab.fing" type="mei:mei_data.FINGER.FRET">
         <xs:annotation>
            <xs:documentation>Indicates which finger, if any, should be used to play an individual string. The index, middle, ring, and little fingers are represented by the values 1-4, while 't' is for the thumb. The values 'x' and 'o' indicate muffled and open strings, respectively.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:attributeGroup>
   <xs:attributeGroup name="mei_att.stringtab.attribute.tab.fret">
      <xs:attribute name="tab.fret" type="mei:mei_data.FRETNUMBER">
         <xs:annotation>
            <xs:documentation>Records the location at which a string should be stopped against a fret.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:attributeGroup>
   <xs:attributeGroup name="mei_att.stringtab.attribute.tab.string">
      <xs:attribute name="tab.string" type="mei:mei_data.STRINGNUMBER">
         <xs:annotation>
            <xs:documentation>Records which string is to be played.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:attributeGroup>
   <xs:attributeGroup name="mei_att.stringtab.position.attributes">
      <xs:attributeGroup ref="mei:mei_att.stringtab.position.attribute.tab.pos"/>
   </xs:attributeGroup>
   <xs:attributeGroup name="mei_att.stringtab.position.attribute.tab.pos">
      <xs:attribute name="tab.pos" type="xs:positiveInteger">
         <xs:annotation>
            <xs:documentation>Records fret position.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:attributeGroup>
   <xs:attributeGroup name="mei_att.stringtab.tuning.attributes">
      <xs:attributeGroup ref="mei:mei_att.stringtab.tuning.attribute.tab.strings"/>
   </xs:attributeGroup>
   <xs:attributeGroup name="mei_att.stringtab.tuning.attribute.tab.strings">
      <xs:attribute name="tab.strings">
         <xs:annotation>
            <xs:documentation>Provides a *written* pitch and octave for each open string or course of strings.</xs:documentation>
         </xs:annotation>
         <xs:simpleType>
            <xs:restriction>
               <xs:simpleType>
                  <xs:list>
                     <xs:simpleType>
                        <xs:restriction base="xs:token">
                           <xs:pattern value="[a-g][0-9](s|f|ss|x|ff|xs|sx|ts|tf|n|nf|ns|su|sd|fu|fd|nu|nd|1qf|3qf|1qs|3qs)?([a-g][0-9](s|f|ss|x|ff|xs|sx|ts|tf|n|nf|ns|su|sd|fu|fd|nu|nd|1qf|3qf|1qs|3qs)?)*"/>
                        </xs:restriction>
                     </xs:simpleType>
                  </xs:list>
               </xs:simpleType>
               <xs:minLength value="1"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
   </xs:attributeGroup>
   <xs:element name="barre">
      <xs:annotation>
         <xs:documentation>A barre in a chord tablature grid.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:attributeGroup ref="mei:mei_att.common.attributes"/>
         <xs:attributeGroup ref="mei:mei_att.startEndId.attributes"/>
         <xs:attribute name="fret">
            <xs:annotation>
               <xs:documentation>Records the location at which the strings should be stopped against a fret in a fretboard diagram. This may or may not be the same as the actual location on the fretboard of the instrument in performance.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:positiveInteger">
                  <xs:minInclusive value="1"/>
                  <xs:maxInclusive value="5"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
